<?import javafx.scene.control.*?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<FlowPane fx:id="root" fx:controller="PercentFat" xmlns:fx="http://javafx.com/fxml">
    <stylesheets>
    <URL value="@Main.css" />
    </stylesheets>
    <children>
    <VBox>
        <Label>Fat</Label>
        <TextField fx:id="firstField"></TextField>
    </VBox>
    <VBox>
        <Label>Calories</Label>
        <TextField fx:id="secondField"></TextField>
    </VBox>
    <VBox>
        <Label>Percentage</Label>
        <TextField fx:id="thirdField"></TextField>
    </VBox>
    <VBox>
        <Button fx:id="buttonhandle" onAction="#actionButton">Click</Button>
    </VBox>
    </children>
</FlowPane>